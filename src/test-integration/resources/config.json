{
  "backend": {
    "file": {
      "path": "/vault/file"
    }
  },
  "listener": {
    "tcp": {
      "address" : "127.0.0.1:8200",
      "tls_cert_file" : "/vault/config/ssl/root-ca.crt",
      "tls_key_file" : "/vault/config/ssl/private.key"
    }
  },
  "default_lease_ttl" : "168h",
  "max_lease_ttl" : "720h",
  "disable_mlock" : true
}
